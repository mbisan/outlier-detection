#!/bin/bash

#SBATCH --nodes=1
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=8
#SBATCH --mem=64GB
#SBATCH --gres=gpu:1
#SBATCH --constraint=rtx3090
#SBATCH --time=1-00:00:00
#SBATCH --job-name=pretrain
#SBATCH --output=O-%x.%j.out
#SBATCH --error=E-%x.%j.err

cd /scratch/mbikandi/outlier-detection

source /scratch/mbikandi/.bashrc
source activate dev2

python3 ood_train.py --dataset SHIFT --alpha 1 --horizon 0 --blur 3 --beta 0.005 --beta2 0.0005 --checkpoint ./test_shift/exp2 --epochs 10 --histogram
python3 ood_train.py --dataset SHIFT --alpha 1 --horizon 0 --blur 3 --beta 0.005 --beta2 0.0 --checkpoint ./test_shift/exp3 --epochs 10 --histogram
